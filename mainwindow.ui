	margin: 15px 0px 10px;
}
/* Hover effect for Song Item */
QListWidget::item{
	border-radius: 10%;
}
QListWidget::item:hover {
    background-color: #1c1c1c; /* Light green aura */
}

/* Image label */
QLabel#pictureLabel {
    border-radius: 50%; /* Circular image */
}

/* Song name styling */
QLabel#nameLabel {
    font-weight: bold;
    font-size: 14px;
    color: #ffffff; /* White text for song name */
}

/* Author name styling */
QLabel#authorLabel {
    font-size: 12px;
    color: #aaaaaa; /* Light grey for author name */
}


/* Heart button styling */
QPushButton#favoriteButton {
    background: none;
    border: none;
    qproperty-icon: url(:/icons/heartunfilled.svg); /* Default unfilled heart */
    qproperty-iconSize: 22px;
}

/* Change heart icon when checked (filled) */
QPushButton#favoriteButton:checked {
    qproperty-icon: url(:/icons/heartfilled.svg); /* Filled heart */
}

QScrollBar:vertical, QScrollBar:horizontal {
    background: transparent; /* Hide scrollbar background */
    width: 0px; /* Completely hide the scrollbar */
    height: 0px; /* Completely hide the scrollbar */
}

