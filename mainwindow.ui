               <property name="styleSheet">
                <string notr="true">/* Groove - the track */
QSlider::groove:horizontal {
    height: 6px;
    background: #404040; /* Dark gray for the track background */
    border: none;
    border-radius: 3px;
}

/* Progress bar - before the handle */
QSlider::sub-page:horizontal {
    background: #1DB954; /* Spotify green for the progress */
    border: none;
    height: 6px;
    border-radius: 3px;
}

/* Remaining bar - after the handle */
QSlider::add-page:horizontal {
    background: #404040; /* Dark gray for the remaining part */
    border: none;
    height: 6px;
    border-radius: 3px;
}

/* Handle - the draggable knob */
QSlider::handle:horizontal {
    background: #FFF; /* White handle for elegance */
    border: 1px solid #1DB954; /* Subtle border for Spotify green */
    width: 16px;
    height: 16px;
    margin: -6px 0; /* Align with the track */
    border-radius: 8px; /* Perfectly circular */
}

/* Handle hover effect */
QSlider::handle:horizontal:hover {
    background: #1DB954; /* Spotify green on hover */
    border: 1px solid #FFF; /* Invert the border for contrast */
}

/* Handle focus (for accessibility) */
QSlider::handle:horizontal:focus {
    border: 2px solid #1DB954; /* Stronger border on focus */
    background: #FFF; /* Keep the handle visible */
}
